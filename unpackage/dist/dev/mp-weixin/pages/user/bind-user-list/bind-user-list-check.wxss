.container {
  padding-bottom: 100rpx;
}
.public-card {
  display: block;
  padding-top: 0;
}
/* 头部部分 */
.box-list-top {
  width: 100%;
  border-bottom: 1px solid #F6F6F6;
}
.box-list-top .btn {
  width: 100%;
  padding: 30rpx 20rpx 30rpx 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.box-list-top .btn .add-btn {
  width: 40%;
  padding: 15rpx 20rpx;
  border-radius: 50rpx;
  text-align: center;
}
/* 中间部分 */
.check-list-check-box {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  border-bottom: 1px solid #F6F6F6;
}
.check-list-check-box checkbox {
  -webkit-box-flex: 8;
  -webkit-flex: 8;
          flex: 8;
  padding-left: 40rpx;
}
/* checkbox 选中的颜色 */
.tui-checkbox .wx-checkbox-input {
  border: 1px solid #8A8A8A;
}
.tui-checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background-color: #4E77FF;
}
.check-list-check-box checkbox label,
.check-list-check-box checkbox view {
  display: block;
  font-size: 34rpx;
  padding: 40rpx 0 0rpx 50rpx;
}
.check-list-check-box checkbox view {
  padding-top: 0;
  padding-bottom: 40rpx;
  font-size: 28rpx;
  color: #8A8A8A;
}
.check-list-check-box ._i {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
}
.check-list-check-box .iconfont {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 42rpx;
  color: #8A8A8A;
}
.bottom-box {
  position: fixed;
  bottom: 50rpx;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.bottom-box .fixed-btn {
  width: 90%;
  text-align: center;
  padding: 20rpx;
  font-size: 34rpx;
  border-radius: 50rpx;
}

