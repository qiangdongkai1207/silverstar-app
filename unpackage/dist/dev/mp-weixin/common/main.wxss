/*<!-- ————————————  单边阴影效果  —————————————————————— -->*/
.flex {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.flex-item {
	margin-right: 30px;
}
.boxshadow12 {
	box-shadow: inset 0px 15px 10px -15px #000;
}
/*上边内阴影示例*/
.boxshadow22 {
	box-shadow: inset -15px 0px 10px -15px #000;
}
/*右边内阴影示例*/
.boxshadow32 {
	box-shadow: 0px 12px 8px -12px #000;
